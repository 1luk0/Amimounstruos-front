<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/llaveabierta"
    tools:context=".Screens.Games.MedioAmbiente.Nivel2.LlaveAbiertaActivity">

  <ImageButton
      android:id="@+id/llave"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:background="@android:color/transparent"
      android:contentDescription="@string/llave"
      app:layout_constraintHeight_percent="0.12"
      app:layout_constraintStart_toStartOf="@+id/guideline150"
      app:layout_constraintTop_toTopOf="@+id/guideline148"
      app:layout_constraintWidth_percent="0.1" />

  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/guideline148"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      app:layout_constraintGuide_begin="0dp"
      app:layout_constraintGuide_percent="0.6" />

  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/guideline150"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="vertical"
      app:layout_constraintGuide_begin="0dp"
      app:layout_constraintGuide_percent="0.215" />


</androidx.constraintlayout.widget.ConstraintLayout>
