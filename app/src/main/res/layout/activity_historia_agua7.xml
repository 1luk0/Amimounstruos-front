<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
  android:background="@drawable/historiaagua7"
    tools:context=".Screens.Games.MedioAmbiente.Nivel1.HistoriaAgua7Activity">

  <ImageButton
    android:id="@+id/botonContinuar"
    android:layout_width="170dp"
    android:layout_height="53dp"
    android:background="@drawable/vamosbt"
    android:contentDescription="@string/cursos"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHeight_percent="0.13"
    app:layout_constraintHorizontal_bias="0.499"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintWidth_percent="0.23"
    tools:layout_editor_absoluteY="351dp" />

  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline80"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    app:layout_constraintGuide_begin="78dp"
    app:layout_constraintGuide_percent="0.13" />

  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline82"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    app:layout_constraintGuide_begin="594dp"
    app:layout_constraintGuide_percent="0.99" />

  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline78"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_begin="931dp"
    app:layout_constraintGuide_percent="0.97" />

  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline79"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    app:layout_constraintGuide_end="358dp" />

  <ImageButton
    android:id="@+id/configurationsButton"
    android:layout_width="55dp"
    android:layout_height="58dp"
    android:layout_marginEnd="4dp"
    android:background="@drawable/mapbutt"
    android:contentDescription="@string/ap2"
    app:layout_constraintBottom_toTopOf="@+id/guideline80"
    app:layout_constraintEnd_toStartOf="@+id/guideline81"
    app:layout_constraintHeight_percent="0.1"
    app:layout_constraintWidth_percent="0.06" />

  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline81"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_begin="931dp"
    app:layout_constraintGuide_percent="0.97" />

</androidx.constraintlayout.widget.ConstraintLayout>
