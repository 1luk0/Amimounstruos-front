<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
  android:background="@drawable/banerabk6"
    tools:context=".Screens.Games.MedioAmbiente.Nivel4.Banera6Activity">


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline208"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="0dp"
        app:layout_constraintGuide_percent="0.14" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline211"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="0dp"
        app:layout_constraintGuide_percent="0.7" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline209"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="0dp"
        app:layout_constraintGuide_percent="0.97" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline210"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="0dp"
        app:layout_constraintGuide_percent="0.96" />

    <ImageButton
        android:id="@+id/continueArrow"
        android:layout_width="202dp"
        android:layout_height="130dp"
        android:background="@drawable/flecharoja"
        android:contentDescription="@string/continuar"
        app:layout_constraintBottom_toTopOf="@+id/guideline211"
        app:layout_constraintEnd_toStartOf="@+id/guideline210"
        app:layout_constraintHeight_percent="0.08"
        app:layout_constraintWidth_percent="0.13"
        tools:layout_constraintHeight_percent="0.18" />

    <ImageButton
        android:id="@+id/mapButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/mapbutt"
        android:contentDescription="@string/mapa"
        app:layout_constraintBottom_toTopOf="@+id/guideline208"
        app:layout_constraintEnd_toStartOf="@+id/guideline209"
        app:layout_constraintHeight_percent="0.09"
        app:layout_constraintWidth_percent="0.06" />

</androidx.constraintlayout.widget.ConstraintLayout>
