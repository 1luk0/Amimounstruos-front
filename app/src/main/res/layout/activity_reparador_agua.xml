<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/reparadoragua"
    tools:context=".Screens.Games.MedioAmbiente.Nivel2.ReparadorAguaActivity">

  <ImageButton
      android:id="@+id/mapButton"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:background="@drawable/mapbutt"
      android:contentDescription="@string/mapa"
      app:layout_constraintBottom_toTopOf="@+id/guideline158"
      app:layout_constraintHeight_percent="0.12"
      app:layout_constraintStart_toStartOf="@+id/guideline157"
      app:layout_constraintWidth_percent="0.065" />

  <ImageButton
      android:id="@+id/botonContinuar"
      android:layout_width="267dp"
      android:layout_height="0dp"
      android:background="@drawable/continuarbt"
      android:contentDescription="@string/continuar"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHeight_percent="0.18"
      app:layout_constraintHorizontal_bias="0.5"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="@+id/guideline159"
      app:layout_constraintWidth_percent="0.3" />

  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/guideline157"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="vertical"
      app:layout_constraintGuide_begin="0dp"
      app:layout_constraintGuide_percent="0.89" />

  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/guideline158"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      app:layout_constraintGuide_begin="0dp"
      app:layout_constraintGuide_percent="0.17" />

  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/guideline159"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      app:layout_constraintGuide_begin="0dp"
      app:layout_constraintGuide_percent="0.36" />

</androidx.constraintlayout.widget.ConstraintLayout>
