<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
  android:background="@drawable/historiaagua6"
    tools:context=".Screens.Games.MedioAmbiente.Nivel1.HistoriaAgua6Activity">

  <ImageButton
    android:id="@+id/botonContinuar"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:background="@drawable/continuarbt"
    android:contentDescription="@string/ap2"
    app:layout_constraintBottom_toTopOf="@+id/guideline82"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHeight_percent="0.15"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintWidth_percent="0.25" />

  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline82"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    app:layout_constraintGuide_begin="594dp"
    app:layout_constraintGuide_percent="0.99" />

  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline78"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_begin="931dp"
    app:layout_constraintGuide_percent="0.97" />

  <androidx.constraintlayout.widget.Guideline
      android:id="@+id/guideline79"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      app:layout_constraintGuide_begin="78dp"
      app:layout_constraintGuide_percent="0.15" />

  <ImageButton
    android:id="@+id/configurationsButton"
    android:layout_width="60dp"
    android:layout_height="50dp"
    android:background="@drawable/mapbutt"
    android:contentDescription="@string/ap2"
    app:layout_constraintBottom_toTopOf="@+id/guideline79"
    app:layout_constraintEnd_toStartOf="@+id/guideline81"
    app:layout_constraintHeight_percent="0.1"
    app:layout_constraintWidth_percent="0.06" />

  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline81"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_begin="931dp"
    app:layout_constraintGuide_percent="0.97" />

</androidx.constraintlayout.widget.ConstraintLayout>
